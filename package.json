{
  "name": "cjko-draw",
  "version": "1.0.0",
  "description": "A collaborative drawing application",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "postinstall": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Corey J. Kothenbeutel",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.0.0",
    "axios": "^0.18.0",
    "dot-env": "0.0.1",
    "express": "^4.16.3",
    "formik": "^2.1.5",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "p5": "^1.1.9",
    "react-p5": "^1.3.6",
    "typescript": "^4.0.2",
    "yup": "^0.29.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cjkook/cjko-draw.git"
  },
  "keywords": [
    "p5",
    "react",
    "art"
  ],
  "bugs": {
    "url": "https://github.com/cjkook/cjko-draw/issues"
  },
  "homepage": "https://github.com/cjkook/cjko-draw#readme"
}
